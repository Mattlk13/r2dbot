{
  "name": "r2dbot",
  "version": "0.0.1",
  "description": "Slack Bot",
  "keywords": [
    "Slack",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gauravChaturvedi/r2dbot"
  },
  "main": "lib/r2dbot.js",
  "bin": {
    "norrisbot": "bin/bot.js"
  },
  "scripts": {
    "start": "node bin/bot.js",
    "test": "node_modules/.bin/xo data/jokesFetcher.js bin/bot.js lib/norrisbot.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^1.4.1",
    "lodash": "^4.14.1",
    "progress": "^1.1.8",
    "request": "^2.60.0",
    "slackbots": "^0.3.0",
    "sqlite3": "^4.0.0",
    "snyk": "^1.83.0"
  },
  "devDependencies": {
    "xo": "^0.5.2"
  },
  "xo": {
    "space": 4,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  },
  "snyk": true
}
